    <link rel="icon" type="image/png" href="~/assets/img/favicon.webp">
</head>
<body class="bg-bunk loaded">

    <!--  Preloader Start -->
    <div class="loader-wrapper">
        <div class="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <!--  Preloader End -->
    <!-- Breadcrumb Section Start -->
    <div class="breadcrumb-wrap bg-mild position-relative">
        <img src="~/assets/img/breadcrumb/br-shape-1.webp" alt="Image" class="br-shape-one position-absolute sm-none bounce">
        <img src="~/assets/img/breadcrumb/br-shape-2.webp" alt="Image" class="br-shape-two position-absolute sm-none moveHorizontal">
        <img src="~/assets/img/breadcrumb/br-shape-3.webp" alt="Image" class="br-shape-three position-absolute top-0 end-0 h-100">
        <div class="container text-center ps-xxl-4">
            <h2 class="br-title text-white">ورود</h2>
            <ul class="br-menu list-unstyle">
                <li class="fs-xxl-20">رستوران ایتار</li>
            </ul>
        </div>
    </div>
    <!-- Breadcrumb Section End -->
    <!-- Account section Start -->
    <div class="container style-two pt-130 pb-100">
        <div class="row align-items-start gx-xxl-45">
            <div class="col-md-6">
                <div class="account-box bg-mild round-10 mb-30">
                    <h4 class="fs-22 text-white f-primary mb-20">ورود</h4>
                    <form method="post">
                        <div class="form-group mb-20">
                            <label for="email" class="text-white d-block mb-10 fw-bold">نام کاربری</label>
                            <input required asp-for="@Model.UserName" class="bg-bunk h-52 w-100 border-0 text-para">
                        </div>
                        <div class="form-group mb-20">
                            <label for="email" class="text-white d-block mb-10 fw-bold">رمز عبور*</label>
                            <input required asp-for="@Model.Password" type="password" class="bg-bunk h-52 w-100 border-0 text-para">
                        </div>

                        <button class="btn style-one" type="submit">ورود</button>
                    </form>
                </div>
            </div>

        </div>
    </div>
    <!-- Account section End -->
    <!-- Back to Top -->
    <button type="button" id="backtotop" class="position-fixed text-center border-0 p-0" style="opacity: 1;">
        <i class="ri-arrow-up-line"></i>
    </button>

    <!-- Link of JS files -->
    <script data-cfasync="false" src="../../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
